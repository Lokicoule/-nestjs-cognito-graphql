{
  "name": "nestjs-cognito-graphql",
  "version": "0.1.0",
  "description": "NestJS Cognito for GraphQL",
  "author": "Loik Fekkai",
  "license": "MIT",
  "url": "https://github.com/Lokicoule/nestjs-cognito-graphql#readme",
  "keywords": [
    "nestjs",
    "cognito",
    "graphql"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"{lib,test}/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0",
    "@nestjs/graphql": "^10.0.16",
    "nestjs-cognito": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.16",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^8.0.1",
    "install": "^0.13.0",
    "jest": "^27.0.6",
    "nestjs-cognito": "^0.1.1",
    "npm": "^8.13.1",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lokicoule/nestjs-cognito-graphql"
  },
  "bugs": "https://github.com/Lokicoule/nestjs-cognito-graphql/issues"
}
